{
    "manifest_version": 3,
    "name": "AI Model Downloader",
    "version": "1.0",
    "background": {
      "service_worker": "background.js"
    },
    
    "permissions": [
      "aiLanguageModelOriginTrial",
      "storage",
      "activeTab"
    ],
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
    }
  }
  }
  